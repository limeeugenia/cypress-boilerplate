{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "dom"],
    "types": ["cypress", "node"],
    "module": "commonjs",
    "strictNullChecks": true,
    "baseUrl": ".",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "allowJs": true,
    "paths": {
        "@fixtures/*": ["./cypress/fixtures/*"],
        "@support/*": ["./cypress/support/*"],
    }
  },
  "include": ["**/*.js", "**/*.ts", "fixtures/*.json"]
}
